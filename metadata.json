{
  "name": "stm-sendmail",
  "version": "0.2.0",
  "author": "stm",
  "summary": "Install and manage the Sendmail MTA.",
  "license": "BSD-2-Clause",
  "source": "git@github.com:smoeding/puppet-sendmail.git",
  "project_page": "https://github.com/smoeding/puppet-sendmail",
  "issues_url": "https://github.com/smoeding/puppet-sendmail/issues",
  "tags": [ "mail", "email", "mta", "sendmail", "smtp" ],
  "operatingsystem_support": [
    {
      "operatingsystem": "Debian",
      "operatingsystemrelease": [ "7", "8" ]
    },
    {
      "operatingsystem": "Ubuntu",
      "operatingsystemrelease": [ "14.04", "15.04" ]
    }
  ],
  "dependencies": [
    {
      "name": "puppetlabs-stdlib",
      "version_requirement": ">= 4.2.0 < 5.0.0"
    },
    {
      "name": "puppetlabs-concat",
      "version_requirement": ">= 1.2.0 < 2.0.0"
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.2.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 3.0.0"
    }
  ]
}
